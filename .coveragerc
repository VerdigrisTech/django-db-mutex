[run]
branch = True
omit =
    db_mutex/migrations/*
    db_mutex/south_migrations/*
    db_mutex/version.py
    db_mutex/apps.py
source = db_mutex
[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError
fail_under = 100
show_missing = 1
